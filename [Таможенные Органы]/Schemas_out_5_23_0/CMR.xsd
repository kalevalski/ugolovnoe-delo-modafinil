<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="urn:customs.ru:Information:TransportDocuments:Car:CMR:5.23.0" version="5.23.0.0" xmlns:catTrans_cu="urn:customs.ru:Information:TransportDocuments:CUTransportCommonAgregateTypesCust:5.22.0" xmlns:catTrans_ru="urn:customs.ru:Information:TransportDocuments:TransportCommonAgregateTypesCust:5.23.0" xmlns:cat_ru="urn:customs.ru:CommonAggregateTypes:5.22.0" xmlns:clt_ru="urn:customs.ru:CommonLeafTypes:5.10.0" xmlns:cmr="urn:customs.ru:Information:TransportDocuments:Car:CMR:5.23.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="urn:customs.ru:CommonLeafTypes:5.10.0" schemaLocation="CommonLeafTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:CommonAggregateTypes:5.22.0" schemaLocation="CommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:TransportDocuments:TransportCommonAgregateTypesCust:5.23.0" schemaLocation="TransportCommonAggregateTypesCust.xsd" />
  <xs:import namespace="urn:customs.ru:Information:TransportDocuments:CUTransportCommonAgregateTypesCust:5.22.0" schemaLocation="CUTransportCommonAggregateTypesCust.xsd" />
  <xs:annotation>
    <xs:documentation>Автодорожная накладная (CMR)</xs:documentation>
  </xs:annotation>
  <xs:element name="CMR" type="cmr:CMRType" />
  <xs:complexType name="CMRType">
    <xs:annotation>
      <xs:documentation>Автодорожная накладная</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="cat_ru:BaseDocType">
        <xs:sequence>
          <xs:element name="LanguageCode" type="clt_ru:LanguageCodeType">
            <xs:annotation>
              <xs:documentation>Код языка документа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="CarrierNotice" type="clt_ru:FreeText250Type">
            <xs:annotation>
              <xs:documentation>Оговорки и замечания перевозчика (гр.18)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="GoodsQuantity" type="clt_ru:QuantityBasisType">
            <xs:annotation>
              <xs:documentation>Общее количество упакованного груза </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="GoodsCost" type="clt_ru:AmountType">
            <xs:annotation>
              <xs:documentation>Объявленная стоимость груза</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StampSign" type="clt_ru:IndicatorType">
            <xs:annotation>
              <xs:documentation>Признак присутствия штампа</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RegistrationDocument" type="cmr:RegistrationDocumentType">
            <xs:annotation>
              <xs:documentation>Сведения о составлении накладной</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignor" type="cmr:CMRConsignorType">
            <xs:annotation>
              <xs:documentation>Сведения о грузоотправителе (гр.1)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Consignee" type="cat_ru:OrganizationType">
            <xs:annotation>
              <xs:documentation>Сведения о грузополучателе (гр.2)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Carrier" type="cmr:CMRCarrierType">
            <xs:annotation>
              <xs:documentation>Сведения о перевозчике (гр.16)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="DeliveryPlace" type="cat_ru:AddressType">
