        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="NaturalPersonIndicator" type="clt_ru:IndicatorType">
        <xs:annotation>
          <xs:documentation>Индикатор, заполняется в случае, если страхователь - физическое лицо</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
